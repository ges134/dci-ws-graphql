{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts", "coverage/lcov-report/*.js"]
  },
  "rules": {
    "no-implicit-dependencies": [true, "dev"],
    "object-literal-sort-keys": false,
    "ordered-imports": false
  }
}